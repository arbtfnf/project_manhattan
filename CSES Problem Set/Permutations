//https://cses.fi/problemset/task/1070

#include <bits/stdc++.h>
using namespace std;
 
int main() {
	long long int n;
	cin>>n;
	if(n==1) cout << 1;
	else if(n<4){
		cout << "NO SOLUTION";
	}else{
		long long int k;
		k = n/2;
		for(long long int i=1; i<=n; i++){
			long long int m = k + i;
			cout<<m<<" ";
			cout<<i<<" ";
			if(i==k)	break;
		}
		if(n%2 == 1) cout << n;
	}
	return 0;
}
